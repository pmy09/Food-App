                    <div class="pcoded-content">
                        <div class="pcoded-inner-content">
                            <!-- Main-body start -->
                            <div class="main-body">
                                <div class="page-wrapper">
                                    <!-- Page-header start -->
                                    <div class="page-header card">
                                        <div class="row align-items-end">
                                            <div class="col-lg-8">
                                                <div class="page-header-title">
                                                    <i class="icofont icofont-user bg-c-blue card1-icon"></i>
                                                    <div class="d-inline">
                                                        <h4>AmaliEats Users</h4>
                                                        
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-lg-4">
                                                <div class="page-header-breadcrumb">
                                                   <ul class="breadcrumb-title">
                                                    <li class="breadcrumb-item">
                                                        <a href="/manager">
                                                            <i class="icofont icofont-home"></i>
                                                        </a>
                                                    </li>
                                                    <li class="breadcrumb-item"><a href="#!">AmaliEats Users</a>
                                                    </li>
                                                    
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- Page-header end -->
                                
                                <!-- Page-body start -->
                                <div class="page-body">
                                    <!-- Basic table card start -->
                                    
                                    <div class="card">
                                        <div class="card-header">
                                            <h5>Users</h5>
                                        <h1><%= user.length %> </h1>    
                                        </div>
                                        <div class="allusers">

        
                                            <div> <h3>Admin </h3></div>
                                            <div> <h3>Staff </h3></div>
                                            <div> <h3> Trainee </h3></div>
                                            <div> <h3> Managers </h3></div>
                                            <div> <h3>Total </h3></div>
                                    
                                            <div> <h3> <%= adminCounter %> </h3></div>
                                            <div> <h3> <%= staffCounter %>  </h3></div>
                                            <div> <h3> <%= traineeCounter %>  </h3></div>
                                            <div> <h3> <%= managerCounter %>   </h3></div>
                                            <div> <h3> <%= user.length %> </h3></div>
                                        </div>
                                        
                                        <div class="card-block table-border-style">
                                            <div class="table-responsive">
                                                <% if (user.length > 0) { %>
                                                <table class="table table-hover">
                                                    <thead>
                                                        <tr>
                                                            
                                                            <th>ID</th>
                                                            <th>Username</th>
                                                            <th>Role</th>
                                                            <th>Email</th>
                                                            <th></th>
                                                            <th></th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <% for (var people = 0; people < user.length; people++) { %>
                                                            <tr>
                                    
                                                                <td><%= user[people]._id %></td>
                                                                <td><%= user[people].username %></td>
                                                                <td><%= user[people].role %> </td>
                                                                <td><%= user[people].email %></td>
                                                                <td><button>Approve</button></td>
                                                                <td><form action="/deluser" method="GET"> <button id="<%= user[people]._id %>" type="submit" class="remove">Remove</button></form></td>
                                                                <!-- <td><button class="remove">Remove</button></td> -->
                                                                
                                                            </tr>
                                                            <% } %>  



                                                    </tbody>
                                                </table>
                                                <% } %> 
                                            </div>
                                        </div>
                                    </div>
                                    
                                </div>
                                <!-- Page-body end -->
                            </div>
                        </div>
                        <!-- Main-body end -->

                        

                        </div>
